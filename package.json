{
  "name": "custom_figma_sprite_sheet_maker",
  "version": "1.0.0",
  "description": "A Figma plugin to create sprite sheets from frames",
  "main": "dist/code.js",
  "scripts": {
    "build": "tsc -p tsconfig.json && npm run build:ui",
    "build:ui": "mkdir -p dist && cp src/ui/ui.html dist/ui.html",
    "watch": "tsc -p tsconfig.json --watch",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": ["figma", "plugin", "sprite-sheet"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@figma/plugin-typings": "^1.82.0",
    "@types/node": "^20.11.24",
    "esbuild": "^0.20.1",
    "typescript": "^5.3.3"
  }
}
